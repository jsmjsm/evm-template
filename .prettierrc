{
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": true,
        "singleQuote": false,
        "bracketSpacing": false,
        "explicitTypes": "preserve"
      }
    },
    {
      "files": "*.js",
      "options": {
        "tabWidth": 4,
        "useTabs": true,
        "semi": false,
        "singleQuote": false,
        "printWidth": 100
      }
    }
  ]
}
